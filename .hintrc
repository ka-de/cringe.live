{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-charset-utf-8": "off",
    "axe/language": [
      "default",
      {
        "html-lang-valid": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off"
      }
    ],
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/structure": [
      "default",
      {
        "definition-list": "off"
      }
    ],
    "apple-touch-icons": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "meta[name=theme-color]"
        ]
      }
    ]
  }
}